generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(cuid())
  password   String    @db.VarChar(32)
  username   String    @unique @db.VarChar(16)
  created_at DateTime  @unique @default(now())
  full_name  String?
  logout_at  DateTime?
}
